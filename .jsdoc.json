{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "pedantic": true,
    "private": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_|node_modules"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
